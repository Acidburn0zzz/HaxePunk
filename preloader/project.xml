<?xml version="1.0" encoding="utf-8"?>
<project>
	<meta title="preloader" package="com.haxepunk.preloader" version="1.0.0" company="HaxePunk"/>

	<app file="Main" main="com.haxepunk.Preloader" path="bin"/>

	<window fps="60" background="0xE6E6E6"/>
	<window width="640" height="480" resizable="false" unless="mobile"/>
	<window width="0" height="0" fullscreen="true" if="mobile"/>
	<window orientation="landscape" vsync="true" antialiasing="0" if="cpp"/>

	<haxelib name="HaxePunk"/>
</project>
